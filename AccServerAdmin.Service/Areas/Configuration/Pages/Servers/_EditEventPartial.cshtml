@model AccServerAdmin.Service.Areas.Configuration.Pages.Servers.EditServerModel

<div class="form-group row">
    <label asp-for="Server.EventCfg.Track" class="col-sm-2 form-control-label">Track</label>
    <div class="col-sm-10">
        <select asp-for="Server.EventCfg.Track" asp-items="Model.Tracks" class="form-control">
            <option value="">-- Select a track --</option>
        </select>
    </div>
</div>

<div class="form-group row">
    <label asp-for="Server.EventCfg.EventType" class="col-sm-2 form-control-label">Event Type</label>
    <div class="col-sm-10">
        <select asp-for="Server.EventCfg.EventType" asp-items="Model.EventTypes" class="form-control">
            <option value="">-- Select an event type --</option>
        </select>
    </div>
</div>

<div class="form-group row">
    <label asp-for="Server.EventCfg.PreRaceWaitingTimeSeconds" class="col-sm-2 form-control-label">Pre Race Wait (Seconds)</label>
    <div class="col-sm-10">
        <input asp-for="Server.EventCfg.PreRaceWaitingTimeSeconds" type="text" class="form-control">
    </div>
</div>

<div class="form-group row">
    <label asp-for="Server.EventCfg.SessionOverTimeSeconds" class="col-sm-2 form-control-label">Session Over Wait (Seconds)</label>
    <div class="col-sm-10">
        <input asp-for="Server.EventCfg.SessionOverTimeSeconds" type="text" class="form-control">
    </div>
</div>

<div class="form-group row">
    <label asp-for="Server.EventCfg.PostQualySeconds" class="col-sm-2 form-control-label">Qually Over Wait (Seconds)</label>
    <div class="col-sm-10">
        <input asp-for="Server.EventCfg.PostQualySeconds" type="text" class="form-control">
    </div>
</div>

<div class="form-group row">
    <label asp-for="Server.EventCfg.PostRaceSeconds" class="col-sm-2 form-control-label">Race Over Wait (Seconds)</label>
    <div class="col-sm-10">
        <input asp-for="Server.EventCfg.PostRaceSeconds" type="text" class="form-control">
    </div>
</div>