@model AccServerAdmin.Service.Areas.Configuration.Pages.Servers.EditServerModel

<div class="form-group row">
    <label asp-for="Server.NetworkCfg.UdpPort" class="col-sm-2 form-control-label">UDP Port</label>
    <div class="col-sm-10">
        <input asp-for="Server.NetworkCfg.UdpPort" type="text" class="form-control">
    </div>
</div>

<div class="form-group row">
    <label asp-for="Server.NetworkCfg.TcpPort" class="col-sm-2 form-control-label">TCP Port</label>
    <div class="col-sm-10">
        <input asp-for="Server.NetworkCfg.TcpPort" type="text" class="form-control">
    </div>
</div>

<div class="form-group row">
    <label asp-for="Server.NetworkCfg.MaxConnections" class="col-sm-2 form-control-label">Max Connections</label>
    <div class="col-sm-10">
        <input asp-for="Server.NetworkCfg.MaxConnections" type="text" class="form-control">
    </div>
</div>


<div class="form-group row">
    <label class="col-sm-2 form-control-label">&nbsp;</label>
    <div class="col-sm-10">
        <div class="i-checks">
            <input asp-for="Server.NetworkCfg.RegisterToLobby" type="checkbox" class="checkbox-template">
            <label asp-for="Server.NetworkCfg.RegisterToLobby" class="label-material">Register to Lobby</label>
        </div>
    </div>
</div>

